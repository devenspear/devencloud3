{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "SENDGRID_API_KEY": "@sendgrid_api_key",
    "EMAIL_FROM": "contact@devenspear.com"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
